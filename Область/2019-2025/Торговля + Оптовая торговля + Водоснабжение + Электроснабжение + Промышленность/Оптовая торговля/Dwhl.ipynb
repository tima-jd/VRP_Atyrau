    "lagr = csv_lagr['0']\n",
    "\n",
    "\n",
    "Dwhl = (Nagr/Nagr.shift(1)/lagr) * 10000\n",
    "Dwhl[0]=105.0643531\n",
    "\n",
    "\n",

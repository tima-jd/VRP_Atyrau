    "from scipy.stats.mstats import gmean\n",
    "import pandas as pd\n",
    "csv = pd.read_csv('vrp_ekz_fact.csv')\n",
    "\n",
    "inf = csv['o_inflation']\n",
    "\n",
    "lelc = csv['o_ifo_energo_makat']\n",
    "Nelc = csv['o_prod_energo_makat']\n",
    "\n",
    "Delc = Nelc/Nelc.shift(1)/lelc *10000\n",
    "\n",
    "Einfelc = (gmean(Delc[1:]))/(gmean(inf[1:]+100))\n",
    "Einfelc"
   ]

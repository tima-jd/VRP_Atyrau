    "import pandas as pd\n",
    "csv = pd.read_csv('vrp_ekz_fact.csv')\n",
    "ltrn = csv['o_ifo_transp']\n",
    "\n",
    "i=18\n",
    "c=100\n",
    "newdf = pd.DataFrame(columns=['c1'], index=range(19))\n",
    "while i>0:\n",
    "    c = (c/ltrn[i]) * 100\n",
    "    i = i-1\n",
    "    newdf.loc[i].c1 = c\n",
    "ltrnS=newdf['c1']\n",
    "ltrnS[18]=100\n",
    "\n",
    "ltrn"
